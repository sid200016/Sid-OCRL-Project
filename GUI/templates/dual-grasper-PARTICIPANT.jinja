<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, width=device-width" />
    <link rel="stylesheet" href="{{ url_for('static', filename='global.css') }}" />
    <link rel="stylesheet" href="{{ url_for('static', filename='dual-grasper-PARTICIPANT.css') }}" />
    <link rel="stylesheet" href="{{ url_for('static', filename='components.css') }}" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Inter:wght@400;600;700&display=swap"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Arial:wght@700&display=swap"
    />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/5.7.0/d3.min.js"></script>
    <script type="text/javascript" src="{{ url_for('static', filename='/send.js') }}"></script>
  </head>
  <script src="{{ url_for('static', filename='/slider_2d.js') }}"></script>
  <body>
    <div class="dual-grasper-participant">

      <div class="gantry-space" id="gantry-space">

        {# gantry curr position #}
        <img 
          id="gantry-marker" 
          alt="" 
          src="{{ url_for('static', filename='/public/gantry.svg') }}" 
        />

        <img 
          class="goal"
          alt=""
          src="{{ url_for('static', filename='/public/goal.png') }}" 
        />

        <img
          class="item-0"
          alt=""
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img 
          class="item-1" 
          alt="" 
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img 
          class="item-2" 
          alt="" 
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img
          class="item-3"
          alt=""
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img
          class="item-4"
          alt=""
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img
          class="item-5"
          alt=""
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img 
          class="item-6" 
          alt="" 
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img 
          class="item-7" 
          alt="" 
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />

        <img 
          class="item-8" 
          alt="" 
          src="{{ url_for('static', filename='/public/no-attempt.svg') }}" 
        />
        
        <svg class="slider-2d" width="840" height="840" style="z-index: 100"></svg>

      </div>
      
      <div class="grasper-controls">
        <h style="top: 3%;">Grasper</h>

        <img 
          class="grasper-icon" 
          alt="" 
          src="{{ url_for('static', filename='/public/dual-grasper.svg') }}" 
        />

        <div class="v-slider slider-l">
          <input type="range" id="range" orient="vertical" step="0.1"/>
          <div class="label">
            <h1 class="value">76%</h1>
            <h2>power</h2>
          </div>
        </div>
        <div class="v-slider slider-r">
          <input type="range" id="range" orient="vertical" step="0.1"/>
          <div class="label">
            <h1 class="value">76%</h1>
            <h2>power</h2>
          </div>
        </div>
      </div>


      <h class="attempts-remaining">10 attempts remaining</div>
      <img
        class="attempts"
        alt=""
        src="{{ url_for('static', filename='/public/attempts.svg') }}" 
      />

      <div class="header-bar">
      
        <img 
          class="g82826-icon4" 
          alt="" 
          src="{{ url_for('static', filename='/public/borg-logo.svg') }}" 
        />

        <img 
          class="g83120-icon4" 
          alt=""
          src="{{ url_for('static', filename='/public/cmu-logo.svg') }}" 
        />

        <img 
          class="rect79742-icon4" 
          alt="" 
          src="{{ url_for('static', filename='/public/rect797422.svg') }}"
        />

        <img 
          class="rect79742-0-icon4" 
          alt="" 
          src="{{ url_for('static', filename='/public/rect7974203.svg') }}"
        />

        <img 
          class="rect79742-0-2-icon4" 
          alt="" 
          src="{{ url_for('static', filename='/public/rect79742022.svg') }}"
        />

        <b class="jan-283">{{date}}</b>
        <b class="am3">{{time}}</b>

      </div>

      <form method="post">
        <div class="mybutton" onmousedown="activate_grasper()">
          <div class="buttontemplate">
            <div class="background"></div>
            <div class="label4">On</div>
          </div>
        </div>
      </form>
    </div>
  </body>
</html>
